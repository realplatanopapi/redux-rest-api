    "prepublish": "npm run build"
  },
  "dependencies": {
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "expect": "^1.20.2",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "nock": "^9.0.9",
    "redux": "^3.6.0",
    "sinon": "^1.17.7"
  }
